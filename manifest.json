{
    "name": "UMaine System Professor Ratings",
    "short_name": "UMS RMP",
    "description": "Displays the professors rating from RateMyProfessors.com on the USM and UMaine course search page for easy access.",
    "version": "1.1.0",
  
    "content_scripts": 
      [

          {
              "matches":["https://online.umaine.edu/course-search*","https://online.umaine.edu/how-to-search-for-courses*"],
              "css":["src/css/extension.css"],
              "js":["src/js/util/util.js","src/js/getDataFromRMP.js","src/js/UMaine.js"],
              "run_at": "document_end"
          },
          {
              "matches":["https://usm.maine.edu/*"],
              "css":["src/css/extension.css"],
              "js":["src/js/util/util.js","src/js/getDataFromRMP.js","src/js/USM.js"],
              "run_at": "document_end"
          }
      ],
    "permissions": [
      "storage",
      "https://search-production.ratemyprofessors.com/*"
    ],
    "icons": {
      "16":"assets/extension_icons/icon16.png",
      "48":"assets/extension_icons/icon48.png",
      "128":"assets/extension_icons/icon128.png" 
    },
    "web_accessible_resources": ["assets/rmp-average.jpg","assets/rmp-good.jpg","assets/rmp-poor.jpg"],
    "manifest_version": 2
  }