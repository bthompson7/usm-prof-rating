{
    "name": "UMaine System Professor Ratings",
    "short_name": "UMAINE-RMP",
    "description": "A Chrome Extension that shows rate my professor ratings directly on the USM and UMaine class search page for easy access",
    "version": "1.0.0",
  
    "content_scripts": 
      [
          {
              "matches":["https://usm.maine.edu/courses*","https://online.umaine.edu/course-search*"],
              "css":["src/css/extension.css"],
              "js":["src/js/getData.js","src/js/extension.js"],
              "run_at": "document_end"
          }
      ],
    "permissions": [
      "storage"
    ],
    "icons": {
      "16":"icons/icon16.png",
      "48":"icons/icon48.png",
      "128":"icons/icon128.png" 
    },
    "manifest_version": 2
  }